# Frontend Environment Variables Example
# Copy this file to .env and fill in your actual values

# API Configuration
REACT_APP_API_BASE=https://your-backend-domain.com

# Firebase Configuration (Replace with your actual Firebase project values)
REACT_APP_FIREBASE_API_KEY=your_firebase_api_key_here
REACT_APP_FIREBASE_AUTH_DOMAIN=your-project-id.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your-project-id
REACT_APP_FIREBASE_STORAGE_BUCKET=your-project-id.firebasestorage.app
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
REACT_APP_FIREBASE_APP_ID=your_firebase_app_id
REACT_APP_FIREBASE_MEASUREMENT_ID=your_measurement_id

# GitHub OAuth Configuration
REACT_APP_GITHUB_CLIENT_ID=Iv23li1MA3ima4RjYmk7

# Development Server Settings (for local development only)
WDS_SOCKET_HOST=localhost
WDS_SOCKET_PORT=3000
WDS_SOCKET_PATH=/ws
FAST_REFRESH=true
GENERATE_SOURCEMAP=false

# Production Settings
BROWSER=none
SKIP_PREFLIGHT_CHECK=true
ESLINT_NO_DEV_ERRORS=true
TSC_COMPILE_ON_ERROR=true
DISABLE_ESLINT_PLUGIN=false